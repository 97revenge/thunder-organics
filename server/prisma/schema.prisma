// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}


// model  post { 
//   // id Int @unique
//   title String 
//   content String 
// } 


model User { 
  id String @id @default(cuid())
  name String 
  lastName String
  email String @unique
  password String 
  createdAt DateTime @default(now())
}





