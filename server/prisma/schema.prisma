// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}




model Product { 
  id Int @id @default(autoincrement()) 
  image String 
  price Int 
  tag String @default("Verduras") 
  name String  
  description String 

  @@map("product")
}




model User { 
  id Int @id @default(autoincrement()) 
  name String
  lastName String
  Email String @unique
  password String 


  @@map("user")
}



